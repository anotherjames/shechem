# Ignore directories generated by Composer
drush/contrib
vendor
web/core
web/modules/contrib
web/libraries
web/themes/contrib
web/sites/sites.php
web/themes/custom/*/scss/base/_sprites.scss

# Ignore paths that contain user-generated content.
web/sites/*/files
web/sites/*/private

# Ignore SimpleTest multi-site environment.
web/sites/simpletest

# Ignore gulp things
node_modules
.sass-cache
.eyeglass_cache
*.css.map

# Other useful ignore stuff
.DS_Store
.DS_Store?
.idea
npm-debug.log

# Ignore config that can be overridden on live.
config/unmanaged/*.yml
config/unmanaged/*/*

# Ignore configuration files that may contain sensitive information.
web/sites/*/settings.local.php
web/sites/settings*.php
web/sites/*services*.yml
web/sites/*/services*.yml
.gitattributes
